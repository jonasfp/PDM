        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cheguei_102"
        tools:targetApi="31">
        <activity
            android:name=".view.listarEncomendas"
