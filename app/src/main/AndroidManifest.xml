        <activity
            android:name=".view.persquisarActivity"
            android:exported="false" />
        <activity
            android:name=".view.listarUsuarios"
            android:exported="false" />
        <activity
            android:name=".view.visualizarActivity"
            android:exported="false" />
        <activity
            android:name=".view.loginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.criarContaActivity" />
        <activity android:name=".view.cadastrarEncomendasActivity" />
